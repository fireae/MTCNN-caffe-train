net: "./PNet_train_all.prototxt"
test_iter: 40
test_interval:2500
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.0001  #0.01
momentum: 0.9
weight_decay: 0.008
# The learning rate policy
lr_policy: "step"
stepsize: 10000 #40000
gamma: 0.1
display: 500
max_iter: 100001
snapshot: 10000 #100000
snapshot_prefix: "MTCNN/det1-all-12"
solver_mode: GPU
